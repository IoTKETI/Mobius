	</xs:simpleType>

	<xs:simpleType name="requestStatus">
		<xs:annotation>
			<xs:documentation>Used for request Status in the &lt;request&gt; resource</xs:documentation>
		</xs:annotation>
        <xs:restriction base="xs:integer">
